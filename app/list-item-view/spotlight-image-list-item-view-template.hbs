{{#each images}}
  <div class="carousel-item clickable" {{{linkTo link}}}>
    <img src="{{getImageUrl this}}" style="display:none">
    <div class="image"
         style="background-image:url('{{getImageUrl this}}')"></div>
    <div class="text-container">
      <div>{{l text}}</div>
    </div>
  </div>
{{/each}}
